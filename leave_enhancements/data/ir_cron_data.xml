<odoo>

	<record id="auto_leave_alloc_cron_sc" model="ir.cron">
		<field name="name">Automatic Leave Allocation - SC</field>
		<field name="model_id" ref="model_automatic_leave_allocation" />
		<field name="state">code</field>
		<field name="code">model._auto_leaves_allocation_sc()</field>
		<field name="interval_number">1</field>
		<field name="interval_type">months</field>
		<field name="numbercall">-1</field>
		<field name="nextcall">2022-05-01 19:05:00</field>
		<field name="doall" eval="True" />
	</record>

	<record id="auto_leave_alloc_cron_an" model="ir.cron">
		<field name="name">Automatic Leave Allocation - Annual</field>
		<field name="model_id" ref="model_automatic_leave_allocation" />
		<field name="state">code</field>
		<field name="code">model._auto_leaves_allocation_an()</field>
		<field name="interval_number">1</field>
		<field name="interval_type">days</field>
		<field name="numbercall">-1</field>
		<field name="doall" eval="True" />
		<field name="nextcall">2022-05-01 19:05:00</field>
	</record>
	
	<record id="auto_leave_alloc_cron_balance" model="ir.cron">
		<field name="name">Automatic Balance Leave Allocation</field>
		<field name="model_id" ref="model_automatic_balance_leave_allocation" />
		<field name="state">code</field>
		<field name="code">model._auto_balance_leaves_allocation()</field>
		<field name="interval_number">1</field>
		<field name="interval_type">days</field>
		<field name="numbercall">-1</field>
		<field name="doall" eval="True" />
		<field name="nextcall">2022-05-01 19:05:00</field>
	</record>
	
<record id="auto_leave_validation" model="ir.cron">
		<field name="name">Automatic Leave Validation</field>
		<field name="model_id" ref="model_hr_leave" />
		<field name="state">code</field>
		<field name="code">model.validate_cron()</field>
		<field name="interval_number">1</field>
		<field name="interval_type">days</field>
		<field name="numbercall">-1</field>
		<field name="doall" eval="True" />
		<field name="nextcall">2022-04-11 18:45:00</field>
	</record>

</odoo>