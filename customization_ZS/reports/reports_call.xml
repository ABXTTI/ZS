<!--    </record>-->

    <record id="attendance_report_pdf1" model="ir.actions.report">
        <field name="name">Attendance Summary</field>
        <field name="model">attendance.report.wizard</field>
        <field name="report_name">attendance_report_odt</field>
        <field name="report_type">py3o</field>
        <field name="report_file">reports/attendance_summary.odt</field>
        <field name="py3o_filetype">odt</field>
        <field name="module">customization_ZS</field>
        <field name="py3o_template_fallback">reports/attendance_summary.odt</field>
        <!-- Add entry in "Print" drop-down list -->
        <field name="binding_type">report</field>
        <field name="binding_model_id" ref="model_attendance_report_wizard"/>
    </record>

</odoo>